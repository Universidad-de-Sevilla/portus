{
  "name": "US/portus",
  "description": "Skeleton for develop web applications based on silex and doctrine at University of Seville",
  "license": "MIT",
  "type": "project",
  "require": {
    "dflydev/doctrine-orm-service-provider": "2.0.*@dev",
    "monolog/monolog": "~1.17",
    "php": ">=5.5.9",
    "silex/silex": "~2.0@dev",
    "silex/web-profiler": "~2.0@dev",
    "symfony/browser-kit": "~2.6",
    "symfony/class-loader": "~2.6",
    "symfony/config": "~2.6",
    "symfony/console": "~2.6",
    "symfony/css-selector": "~2.6",
    "symfony/debug": "~2.6",
    "symfony/doctrine-bridge": "~2.7",
    "symfony/finder": "~2.6",
    "symfony/form": "~2.6",
    "symfony/monolog-bridge": "~2.6",
    "symfony/process": "~2.6",
    "symfony/security": "~2.6",
    "symfony/twig-bridge": "~2.6",
    "symfony/validator": "~2.6",
    "symfony/yaml": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "US\\Portus\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "run": [
      "echo 'Started web server on http://localhost:8888'",
      "php -S localhost:8888 -t web web/index_dev.php"
    ]
  }
}
